<server>

  <featureManager>
    <feature>servlet-3.1</feature>
    <feature>jsp-2.3</feature>
  </featureManager>

  <httpEndpoint id="defaultHttpEndpoint"
    host="*"
    httpPort="9080">
    <tcpOptions soReuseAddr="true"/>
  </httpEndpoint>

  <application name="Selfinformation" context-root="/Selfinformation" location="${appLocation}" type="war"/>

</server>
